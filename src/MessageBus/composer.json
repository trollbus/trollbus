{
    "name": "trollbus/message-bus",
    "description": "Trollbus is Flexible Message Bus implementation with middleware and handler result support",
    "keywords": ["trollbus", "message", "bus", "cqrs", "command", "query", "event"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "require": {
        "php": "8.1.* || 8.2.* || 8.3.* || 8.4.*",
        "trollbus/message": "self.version",
        "psr/clock": "^1.0",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Trollbus\\MessageBus\\": "./"
        }
    },
    "config": {
        "lock": false,
        "vendor-dir": "./../../var/MessageBus/vendor"
    },
    "repositories": {
        "trollbus/message": {
            "type": "path",
            "url": "./../Message"
        }
    }
}